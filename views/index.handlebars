{{!-- CONTACT --}}
<section id="contact">
  <div class="container">
    <h2 class="section-title" style="padding-bottom: 5rem;">Contact</h2>
    <div class="contact-wrapper">
      <div class="contact-wrapper__text">
        <form class="contact-form" action="/" method="POST">
          <input type="text" name="name" placeholder="Name" required />
          <input type="text" name="company" placeholder="Company" required />
          <input type="email" name="email" placeholder="Email" required />
          <input type="text" name="mobile" placeholder="Mobile" required />
          <textarea class="text-area" name="message" rows="6" placeholder="Message" required></textarea>
          <button href="#" class="cta-btn cta-btn--resume" type="submit">Send</button>
        </form>
      </div>
    </div>
  </div>
</section>

